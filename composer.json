{
    "name": "scholieren-com/laravel-typeform",
    "description": "A simple Laravel Facade to retrieve typeform responses and to manage webhooks calls",
    "type": "library",
    "require": {
        "php": "^7.2.5|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/config": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Yo1L\\LaravelTypeForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yo1L\\LaravelTypeForm\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yo1L\\LaravelTypeForm\\TypeFormServiceProvider"
            ],
            "aliases": {
                "TypeForm": "Yo1L\\LaravelTypeForm\\TypeFormFacade"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Yo1L",
            "email": "yleviavant@gmail.com"
        }
    ]
}
